{
  "version": 2,
  "scope": "scope",
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "/db/index.js",
      "use": "@now/node"
    },
    {
      "src": "/dist/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/db/index.js" },
    { "handle": "filesystem" },
    { "src": "/add/(.*)", "dest": "/src/views/add" },
    { "src": "/edit/(.*)", "dest": "/src/views/edit" },
  ]
}

{
  "version": 2,
  "scope": "scope",
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "/db/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/db/index.js" },
    { "handle": "filesystem" },
    { "src": "/usuarios", "dest": "/db/index.js" },
    { "src": "/usuarios/:id", "dest": "/db/index.js" }
  ]
}

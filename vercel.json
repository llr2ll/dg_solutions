{
  "version": 2,
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "/api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "/dist/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "use": "/dist/api/index.js" },
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "use": "/dist" },
    { "src": "/(.*).(js|css|ico)", "use": "/dist/apps/react-app/$1.$2" },
    { "src": "/(.*)", "dest": "/dist/apps/react-app/index.html" }
  ]
}
